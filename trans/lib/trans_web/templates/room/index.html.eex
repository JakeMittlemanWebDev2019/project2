<h1>Available Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for room <- @rooms do %>
    <tr>
      <td><%= room.name %></td>

      <td>
        <span><%= link "Show", to: Routes.room_path(@conn, :show, room) %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Room", to: Routes.room_path(@conn, :new), class: "btn btn-primary" %></span>
